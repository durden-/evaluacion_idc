<% if signed_in? %>
  <table class="front" summary="For signed-in users">
    <tr>
      <td class="main">
        <h1 class="evaluation">Favor de llenar el siguiente formulario:</h1>
        <%= render 'shared/evaluation_form' %>
      </td>
    </tr>
  </table>
<% else %>
  <p>
  <h1>Evaluación de los IDC que auxilian al CNOC.</h1>
  </p>

  <%= link_to "Iniciar sesión", signin_path, :class => "signup_button round" %>
<% end %>
